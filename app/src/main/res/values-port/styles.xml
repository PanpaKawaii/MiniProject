<resources>
    <style name="Title">
        <item name="android:textSize">60sp</item>
    </style>
    <style name="OrientationStyle">
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="HeaderSize">
        <item name="android:layout_height">120dp</item>
    </style>
    <style name="OrientationStyle2">
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">500dp</item>
        <item name="android:layout_height">500dp</item>
        <item name="android:rotation">-90</item>
        <item name="android:layout_marginBottom">20dp</item>
    </style>
    <style name="RotateInput">
        <item name="android:rotation">90</item>
    </style>
    <style name="MoneyButton">
        <item name="android:layout_gravity">bottom|end</item>
    </style>
    <style name="FrameMargin">
        <item name="android:paddingHorizontal">28dp</item>
        <item name="android:clipChildren">false</item>
        <item name="android:clipToPadding">false</item>
    </style>
    <style name="CustomProgressBar" parent="@android:style/Widget.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/custom_progress_drawable</item>
        <item name="android:minHeight">10dp</item>
        <item name="android:maxHeight">10dp</item>
        <item name="android:indeterminateOnly">false</item>
    </style>
</resources>
